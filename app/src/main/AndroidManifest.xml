<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.schetime">

    <application
        android:name="com.alian.schetime.SchetimeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ScheTime">
        <activity
            android:name="com.alian.schetime.ui.activities.auth.SignInActivity"
            android:exported="false" />
        <activity
            android:name="com.alian.schetime.ui.activities.compose.auth.SignInActivityCompose"
            android:exported="false"
            android:label="@string/title_activity_sign_in_compose"
            android:theme="@style/Theme.ScheTime.NoActionBar" />
        <activity
            android:name="com.alian.schetime.ui.activities.compose.MainActivityCompose"
            android:exported="true"
            android:theme="@style/Theme.ScheTime.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.alian.schetime.ui.activities.compose.WelcomeActivityCompose"
            android:exported="false"
            android:label="@string/title_activity_welcome_compose"
            android:theme="@style/Theme.ScheTime.NoActionBar" />
        <activity
            android:name="com.alian.schetime.ui.activities.notes.DetailUpdateNoteActivity"
            android:exported="false" />
        <activity
            android:name="com.alian.schetime.ui.activities.user.EditUserActivity"
            android:exported="false" />
        <activity
            android:name="com.alian.schetime.ui.activities.notes.AddNoteActivity"
            android:exported="false" />
        <activity
            android:name="com.alian.schetime.ui.activities.HomeActivity"
            android:exported="false" />
        <activity
            android:name="com.alian.schetime.ui.activities.auth.SignUpActivity"
            android:exported="false" />
        <activity
            android:name="com.alian.schetime.ui.activities.MainActivity"
            android:exported="false" />
    </application>

</manifest>